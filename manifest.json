{
  "name": "Saco Cheio TV Enchant",
  "version": "0.0.1",
  "description": "descrição fodase",
  "manifest_version": 3,
  "author": "Julio Silva",
  "action": {
    "default_popup": "src/popup/index.html",
    "default_title": "Saco Cheio Enchant"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.sacocheio.tv/*"
      ],
      "js": ["src/content.js"]
    }
  ]
}
